class Solution {
public:
    int removeDuplicates(vector<int>& nums) {
        set<int> st;
        int i = 0;
        for(auto &it : nums){
            if(st.find(it) == st.end()){
                st.insert(it);
                nums[i] = it;
                i++;
            }
        }
        
        return st.size();
    }
};