class Solution {
public:
    void sortColors(vector<int>& nums) {
        map<int,int>mpp;
        for(auto &color : nums){
            mpp[color]++;
        }
        int i = 0;
        for(auto it : mpp){
            int color = it.first;
            int cnt = it.second;
            while(cnt){
                nums[i] = color;
                cnt--;
                i++;
            }
        }
    }
};