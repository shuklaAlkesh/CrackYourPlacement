class Solution {
public:
    vector<int> findDuplicates(vector<int>& nums) {
        int n = nums.size();
        unordered_map<int,int> mpp;
        unordered_set<int> st;
        for(auto &it : nums){
            mpp[it]++;
            if(mpp[it] > 1){
                st.insert(it);
            }
        }

        vector<int>ans(st.begin(),st.end());
        return ans;
    }
};