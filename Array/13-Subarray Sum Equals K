class Solution {
public:
    int subarraySum(vector<int>& nums, int k) {
        int ans = 0;
        unordered_map<int,int>mpp;
        mpp[0] = 1;
        int preSum = 0;
        for(int i=0;i<nums.size();i++){
            preSum += nums[i];
            int remove = preSum - k;
            ans += mpp[remove];
            mpp[preSum] += 1;
        }
        return ans;
    }
};