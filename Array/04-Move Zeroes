class Solution {
public:
    void moveZeroes(vector<int>& nums) {
        int n = nums.size();
        vector<int> ans;
        int count0 = 0;
        for(auto it : nums){
            if(it == 0){
                count0++;
            }else{
                ans.push_back(it);
            }
        }

        while(count0){
            ans.push_back(0);
            count0--;
        }
        nums = ans;
    }
};