class Solution
{
    public:
    Node* reverse(Node* root){
        Node* prev = NULL;
        Node* curr = root;
        
        while(curr){
            Node* front =  curr->next;
            curr->next = prev;
            prev = curr;
            curr = front;
        }
        return prev;
    }
    Node *compute(Node *head)
    {
        head = reverse(head);
        Node* cur = head;
        int maxi = head->data;
        Node* prev = head;
        head = head->next;
        while(head){
            if(head->data >= maxi){
                maxi = head->data;
                prev = head;
                head = head->next;
            }else{
                prev->next = head->next;
                head = prev->next;
            }
        }
        head = reverse(cur);
        return head;
    }
    
};