class Solution {
public:
    string intToRoman(int num) {
        vector<int>intcode = {1000,900,500,400,100,90,50,40,10,9,5,4,1};
        string Romancode[] = {"M","CM","D","CD","C","XC","L","XL","X","IX","V","IV","I"};
        
        string result = "";

        for(int i = 0; i < intcode.size(); i++) {
            while(num >= intcode[i]) {
                result.append(Romancode[i]);
                num -= intcode[i];
            }
        }
        return result;
    }
};