class Solution {
public:
    int f(int n, vector<int> & dp){
        // base case
        if(n<=1) return 1;
        // here we use dynamic programming
        if(dp[n] != -1) return dp[n];
        // Recursion
        return dp[n] = f(n-1,dp) +f(n-2,dp);
    }
    int climbStairs(int n) {
       vector<int> dp(n+1,-1);
        return f(n,dp);
    }
};