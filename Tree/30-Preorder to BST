class Solution {
  public:
    // Function that constructs BST from its preorder traversal.
    Node* solve(int pre[],int& i,int UpBound,int size){
        // base case
        if(i == size || pre[i] > UpBound) return NULL;
        // Recursive Approach
        Node* root = newNode(pre[i++]);
        root->left = solve(pre,i,root->data,size);
        root->right = solve(pre,i,UpBound,size);
        return root;
    }
    Node* Bst(int pre[], int size) {
        int i=0;
        return solve(pre,i,INT_MAX,size);
    }
};